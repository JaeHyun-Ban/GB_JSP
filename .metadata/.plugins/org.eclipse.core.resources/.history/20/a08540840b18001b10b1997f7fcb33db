<%@page import="java.util.Random"%>
<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<%!
	//10의 배수이면 당첨
	public int ran() {
		Random ran = new Random();
		int ranNum = ran.nextInt(40);
		
		return ranNum;
	}
	
%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
</head>
<body>

	<% if(ran()%10 == 0) {%> 
		10의 배수입니다!<br>
		당첨되었습니다
	<%} %>
	
	<!-- 구구단 4단 출력 -->




</body>
</html>